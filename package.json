{
  "name": "ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "create-language": "node create-language",
    "create-router": "node create-router",
    "upxtorem": "node upxtorem",
    "dev": "node server",
    "server": "cross-env NODE_ENV=production node server",
    "build": "cross-env NODE_ENV=production && npm run build:client && npm run build:server && vite build --outDir dist/client --ssr sw.js --emptyOutDir false && node build.pwa",
    "build:client": "vite build --outDir dist/client",
    "build:server": "npm run build:server:base && npm run build:server:config",
    "build:server:base": "vite build --ssr src/entry-server.js --outDir dist/server && node globalVariable",
    "build:server:config": "vite build --outDir dist/server --ssr src/entry-server-config.ts --emptyOutDir false",
    "build:static": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@better-scroll/core": "^2.5.1",
