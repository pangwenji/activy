    "@better-scroll/observe-dom": "^2.5.1",
    "@better-scroll/observe-image": "^2.5.1",
    "@better-scroll/pull-down": "^2.5.1",
    "@better-scroll/pull-up": "^2.5.1",
    "@better-scroll/scroll-bar": "^2.5.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "axios": "^1.5.1",
    "base-64": "^1.0.0",
    "cookie-parser": "^1.4.6",
